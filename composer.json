{
  "name": "contributte/api-router",
  "type": "library",
  "description": "RESTful Router for your Apis in Nette Framework - created either directly or via annotation",
  "keywords": [
    "rest",
    "api",
    "routes",
    "nette",
    "router",
    "restapi",
    "restrouter",
    "routing",
    "route"
  ],
  "homepage": "https://github.com/contributte/api-router",
  "license": [
    "MIT"
  ],
  "authors": [
    {
      "name": "Pavel Janda",
      "homepage": "http://paveljanda.com"
    }
  ],
  "require": {
    "php": ">=8.2",
    "nette/http": "^3.3.0",
    "nette/application": "^3.2.5",
    "nette/di": "^3.2.2",
    "contributte/utils": "^0.6.0 || ^0.7.0",
    "symfony/cache": "^7.0.7",
    "doctrine/annotations": "^2.0.1"
  },
  "require-dev": {
    "contributte/qa": "^0.4",
    "contributte/tester": "^0.4",
    "contributte/phpstan": "^0.1",
    "mockery/mockery": "^1.6.6",
    "tracy/tracy": "^2.10.5"
  },
  "autoload": {
    "psr-4": {
      "Contributte\\ApiRouter\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "6.1.x-dev"
    }
  }
}
